{
  "name": "prev-cli",
  "version": "0.6.0",
  "description": "Transform MDX directories into beautiful documentation websites",
  "type": "module",
  "license": "MIT",
  "bin": {
    "prev": "./dist/cli.js"
  },
  "files": [
    "dist",
    "src/theme",
    "src/ui"
  ],
  "keywords": [
    "documentation",
    "mdx",
    "markdown",
    "cli",
    "static-site",
    "vite",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagz0ne/prev-cli.git"
  },
  "bugs": {
    "url": "https://github.com/lagz0ne/prev-cli/issues"
  },
  "homepage": "https://github.com/lagz0ne/prev-cli#readme",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target node --packages external && tsc --emitDeclarationOnly",
    "dev": "tsc --watch",
    "test": "bun test src",
    "test:integration": "bun run build && bun test test/integration.test.ts",
    "test:all": "bun run test && bun run test:integration"
  },
  "dependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@tanstack/react-router": "^1.145.7",
    "@terrastruct/d2": "^0.1.33",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "fast-glob": "^3.3.0",
    "fumadocs-core": "^16.4.3",
    "fumadocs-ui": "^16.4.3",
    "lucide-react": "^0.460.0",
    "mermaid": "^11.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "rolldown-vite": "^7.3.0",
    "tailwind-merge": "^2.5.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "bun-types": "^1.3.5",
    "typescript": "^5.7.0"
  }
}
